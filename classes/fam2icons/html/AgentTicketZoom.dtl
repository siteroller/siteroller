          <td><b>$Text{"$QData{"TicketFreeTimeKey"}"}:</b></td>
          <td><div title="$TimeShort{"$QData{"TicketFreeTime"}"}">$TimeShort{"$QData{"TicketFreeTime"}"}</div></td>
        </tr>
